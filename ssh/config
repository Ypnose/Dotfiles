# -- OPTIONS -----------------------------------------------------------
ForwardAgent no
HashKnownHosts yes
ServerAliveInterval 60

# -- HOSTS -------------------------------------------------------------
Host local
	HostName 127.0.0.1
	Port 9922
	StrictHostKeyChecking no
	UserKnownHostsFile /dev/null
